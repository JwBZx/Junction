{
  "compilerOptions": {
    "target": "es6",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "pretty": true,
    "removeComments": true,
    "allowJs": false,
    "jsx": "preserve"
  },
  "compileOnSave": true
}
